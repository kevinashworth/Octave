  "scripts": {
    "start": "meteor --port 4004 --settings settings.json",
    "start-debug": "meteor debug --port 4004 --settings settings.json",
    "start-mongo": ". scripts/start-mongo.sh",
    "start-mozart": ". scripts/start-mozart.sh",
    "start-triad": ". scripts/start-triad.sh",
    "start-mongo-old": ". scripts/start-mongo-old.sh",
    "start-visualizer": "meteor --extra-packages bundle-visualizer --production --port 4004 --settings settings.json",
    "prepare": "cd mockaroo; ./mockaroo.sh",
    "prestart": "node .vulcan/prestart_vulcan.js",
    "postinstall": "patch-package; node mockaroo/postinstall.js",
    "lint:fix": "eslint --fix 'packages/octave/**/*.js' 'packages/octave/**/*.jsx'",
    "lint:nofix": "eslint 'packages/octave/**/*.js' 'packages/octave/**/*.jsx' 'tests/cypress/**/*.js'",
    "lint:cypress:fix": "eslint --env mocha --fix 'tests/cypress/**/*.js'",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build:sass": "node-sass 'packages/octave/lib/stylesheets/main.scss' -o 'packages/octave/lib/stylesheets/compiled'",
    "build:sass:min": "node-sass packages/octave/lib/stylesheets/main.scss packages/octave/lib/stylesheets/compiled/main-min.css --output-style compressed",
    "watch:sass": "npm run build:sass && npm run build:sass -w",
    "build-deploy-octave": ". scripts/build-deploy-octave.sh",
    "build-deploy-public": ". scripts/build-deploy-public.sh",
    "build-for-paas1": ". scripts/paas1-build.sh",
    "deploy-to-paas1": ". scripts/paas1-deploy.sh",
    "deploy-to-paas2": ". scripts/paas2-deploy.sh",
    "deploy-to-paas2b": ". scripts/paas2b-deploy.sh",
    "docs": "jsdoc -c jsdoc.json",
    "cypress:open": "cypress open",
    "cypress:headless": "cypress run",
    "start-testing-production": "meteor --port 4004 --settings settings-testing.json --production",
    "start-testing": "meteor --port 4004 --settings settings-testing.json",
    "test": "start-server-and-test start-testing http://localhost:4004 cypress:open",
    "mockaroo": "cd mockaroo; ./mockaroo.sh; cd ..",
    "start-mockaroo": "meteor --port 4004 --settings settings-mockaroo.json",
    "release": "release-it"
  },
