  "scripts": {
    "build-for-heroku": "meteor build ./.build --directory --server-only --architecture=os.linux.x86_64",
    "deploy-to-heroku": "open --hide --background -a Docker && cp Dockerfile ./.build/bundle && heroku container:push web -a sleepy-earth-65615 --context-path ./.build/bundle && heroku container:release web -a sleepy-earth-65615",
    "deploy-to-galaxy": "DEPLOY_HOSTNAME=us-east-1.galaxy-deploy.meteor.com meteor deploy v8.the-4th-wall.com --settings .galaxy/settings.json",
    "deploy-to-meteorapp": "DEPLOY_HOSTNAME=us-east-1.galaxy-deploy.meteor.com meteor deploy v8.meteorapp.com --settings .galaxy/settings.json",
    "prestart": "node .vulcan/prestart_vulcan.js",
    "start": "meteor --port 4004 --settings settings.json",
    "start-debug": "meteor debug --port 4004 --settings settings.json",
    "start-atlas": "./start-atlas.sh",
    "start-mlab": "./start-mlab.sh",
    "start-visualizer": "meteor --extra-packages bundle-visualizer --production --port 4004 --settings settings.json",
    "lint": "standard 'packages/v8/**/*.js' 'packages/v8/**/*.jsx' --fix",
    "lintnofix": "standard 'packages/v8/**/*.js' 'packages/v8/**/*.jsx'",
    "lintnofixv": "standard --verbose 'packages/v8/**/*.js' 'packages/v8/**/*.jsx'",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build:sass": "node-sass 'packages/v8/lib/stylesheets/main.scss' -o 'packages/v8/lib/stylesheets/compiled'",
    "build:sass:min": "node-sass packages/v8/lib/stylesheets/main.scss packages/v8/lib/stylesheets/compiled/main-min.css --output-style compressed",
    "watch:sass": "npm run build:sass && npm run build:sass -w",
    "postinstall": "patch-package"
  },
